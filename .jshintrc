{
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxparams": 3,
  "maxdepth": 2,
  "maxstatements": 20,
  "maxcomplexity": 10,
  "maxlen": 80,

  "expr": true,
  "esnext": true,
  "validthis": true,

  "browser": true,
  "devel": true,
  "node": true,

  "predef": [
    "define",
    "require",
    "describe",
    "it",
    "xit",
    "before",
    "beforeEach",
    "after",
    "afterEach",
    "sinon",
    "expect"
  ]
}
